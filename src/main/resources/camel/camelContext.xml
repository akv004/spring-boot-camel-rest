<?xml version="1.0" encoding="UTF-8"?>

  <routes xmlns="http://camel.apache.org/schema/spring">
  <route>
      <from uri="timer:foo?period=500000"/>
      <log message="#############The message is generated by timer route #############"/>
      <to uri="mock:result"/>
  </route>
  
  
 
  <route>
    <from uri="direct:getimage"/>
      <log message="try to find image ${header.imagename}"/>
